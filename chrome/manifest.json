{
  "name": "Déjà Vu",
  "description": "Find anything you have ever browsed",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "identity",
    "identity.email",
    "https://deja-vu.herokuapp.com/"
  ]
}